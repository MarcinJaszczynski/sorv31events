# Konfiguracja optymalizacji zasobów

# Optymalizacja obrazów
VITE_IMAGE_OPTIMIZATION=true
VITE_IMAGE_QUALITY=85
VITE_IMAGE_WEBP=true

# Optymalizacja CSS
VITE_CSS_MINIFY=true
VITE_CSS_PURGE=true

# Optymalizacja JS
VITE_JS_MINIFY=true
VITE_JS_DROP_CONSOLE=true

# Kompresja
VITE_GZIP_COMPRESSION=true
VITE_BROTLI_COMPRESSION=true

# Analiza bundli
VITE_BUNDLE_ANALYZER=false

# Cache
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=86400
